        @model SchedulerV4.Models.ScheduleNPublEntity

@{
    ViewData["Title"] = "Edit Schedule";
}

<h1>Edit Schedule</h1>

<form asp-action="Edit" method="post">
    @Html.AntiForgeryToken()
    <input type="hidden" asp-for="LESSON_ID" />

    <div class="form-group">
        <label asp-for="GROUPNO"></label>
        <input asp-for="GROUPNO" class="form-control" />
        <span asp-validation-for="GROUPNO" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="GROUPID"></label>
        <input asp-for="GROUPID" class="form-control" />
        <span asp-validation-for="GROUPID" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="DEN"></label>
        <input asp-for="DEN" class="form-control" />
        <span asp-validation-for="DEN" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="VREM"></label>
        <input asp-for="VREM" class="form-control" />
        <span asp-validation-for="VREM" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="DATA"></label>
        <input asp-for="DATA" class="form-control" type="date" />
        <span asp-validation-for="DATA" class="text-danger"></span>
    </div>

    <button type="submit" class="btn btn-primary">Save</button>
</form>
